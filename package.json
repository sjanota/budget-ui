    "react-dom": "^16.9.0",
    "react-hotkeys": "^2.0.0",
    "react-router": "latest",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.1.2",
